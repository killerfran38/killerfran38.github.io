                <datadir zip="true">https://killerfran38.github.io/kodi-repo/</datadir>
            </dir>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>Repositorio de killerfran38</summary>
            <description>Addons, repositorios y skins mantenidos por killerfran38</description>
            <platform>all</platform>
        </extension>
    </addon>

