            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="killerfran38 Repo">
            <dir>
                <info compressed="false">https://killerfran38.github.io/kodi-repo/addons.xml</info>
